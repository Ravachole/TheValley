config_version=5

[application]

config/name="TheValley"
run/main_scene="res://Scenes/the_valley.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"

[debug]

file_logging/enable_file_logging=true
file_logging/enable_file_logging.pc=false
file_logging/log_path="res://Logs"

[display]

window/size/viewport_width=3820
window/size/viewport_height=2400
window/size/mode=2

[dotnet]

project/assembly_name="TheValley"

[global_group]

food=""
water=""

[rendering]

lights_and_shadows/directional_shadow/soft_shadow_filter_quality=3
anti_aliasing/quality/msaa_3d=1
